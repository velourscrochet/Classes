<script type="text/javascript">

function change_modal_frame(url) {
	document.getElementById('modal_login_frame').src = url;
}

$(function() {
    
    var ismobile = navigator.userAgent.match(/(Mobile)|(Android)|(MSIE)/i);

    if (!ismobile) {
        
        // Add mimimal=true and FancyBox it
        $('a.authmethod').each(function() {
            $(this)
                .attr('href', $(this).attr('href') + '&minimal=true')
                .fancybox({
                    width:820,
                    height:450,
                    padding:25,
                    type:'iframe',
                    scrolling:'no',
                    autoScale: false,
                    hideOnContentClick:false,
                    hideOnOverlayClick:false,
                    onComplete:function() { ModalFocus.enable('#fancybox-content'); },
                    onClosed:function() { ModalFocus.disable(); }
                });
            });


    }
    
});

</script>

