\begin_body

\begin_layout Subsection*
Problem 4:
\end_layout

\begin_layout Standard
\begin_inset Formula $\int_{2}^{3}\frac{2}{x^{2}}dx$
\end_inset

 such that 
\begin_inset Formula $n=4$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Standard
Trapezoidal Approximation:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T=\frac{3-2}{2\times4}\times[f(2)+2\times f(2.25)+2\times f(2.5)+2\times f(2.75)+f(3)]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T=\frac{1}{8}\times(.5+2\times.3951+2\times.32+2\times.2645+.2222)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T=.3352
\]

\end_inset


\end_layout

\begin_layout Standard
Simpson's Approximation: 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
S=\frac{3-2}{3\times4}\times[f(2)+4\times f(2.25)+2\times f(2.5)+4\times f(2.75)+f(3)]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
S=\frac{1}{12}\times(.5+4\times.3951+2\times.32+4\times.2645+.2222)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
S=.3333
\]

\end_inset


\end_layout

\begin_layout Standard
Exact value:
\end_layout

\begin_layout Standard

\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none
\begin_inset Formula 
\[
E=\int_{2}^{3}\frac{2}{x^{2}}dx
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E=-\frac{2}{x}|_{2}^{3}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E=\frac{1}{3}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Problem 30:
\end_layout

\begin_layout Standard
Solve for n, such that the error is 
\begin_inset Formula $\leq$
\end_inset

.00001 for the approximation of
\begin_inset Formula $\int_{0}^{1}\frac{1}{x}dx$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Standard
Note that 
\begin_inset Formula $f^{\prime}=\frac{-1}{(x+1)^{2}}$
\end_inset

, 
\begin_inset Formula $f^{\prime\prime}=\frac{2}{(x+1)^{3}}$
\end_inset

, 
\begin_inset Formula $f^{\prime\prime\prime}=\frac{-6}{(x+1)^{4}}$
\end_inset

, and 
\begin_inset Formula $f^{(4)}=\frac{24}{(x+1)^{5}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Standard
Trapezoidal Approximation Error:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
|E|\leq\frac{(b-a)^{3}}{12\times n_{t}^{2}}\times[max|f^{\prime\prime}(x)|_{a}^{b}]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
.00001\leq\frac{(1-0)^{3}}{12\times n_{t}^{2}}\times[max|\frac{2}{(1+x)^{3}}|]
\]

\end_inset


\end_layout

\begin_layout Standard
There are no critical points in 
\begin_inset Formula $f^{\prime\prime}$
\end_inset

in x = [0,1], so 
\begin_inset Formula $max|\frac{2}{(1+x)^{3}}|=f^{\prime\prime}(0)=2$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n_{t}^{2}\geq\frac{1\times2}{12\times.00001}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n_{t}\geq129.099
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n_{t}=130
\]

\end_inset


\end_layout

\begin_layout Standard
Simpson Approximation Error:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
|E|\leq\frac{(b-a)^{5}}{180\times n_{S}^{4}}\times[max|f^{(4)}(x)|_{a}^{b}]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
.00001\leq\frac{(1-0)^{5}}{180\times n_{S}^{4}}\times[max|\frac{24}{(1+x)^{5}}|]
\]

\end_inset


\end_layout

\begin_layout Standard
There are no critical points in 
\begin_inset Formula $f^{(4)}$
\end_inset

in x = [0,1], so 
\begin_inset Formula $[max|\frac{24}{(1+x)^{5}}|]=f^{(4)}(0)=24$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n_{S}^{4}\geq\frac{1\times24}{180\times.00001}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
n_{S}\geq
\]

\end_inset


\end_layout

