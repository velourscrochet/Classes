\begin_body

\begin_layout Subsection*
Problem 24:
\end_layout

\begin_layout Standard
Expand the logarithmic expression:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
ln(xyz)=ln(x\times y\times z)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=ln(x)+ln(y)+ln(z)
\]

\end_inset


\end_layout

\begin_layout Subsection*
Problem 26:
\end_layout

\begin_layout Standard
Expand the logarithmic expression:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
ln\sqrt{a-1}=ln(a-1)^{\frac{1}{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{1}{2}\times ln(a-1)
\]

\end_inset


\end_layout

\begin_layout Subsection*
Problem 34:
\end_layout

\begin_layout Standard
Express as a single logarithm:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
2[ln(x)-ln(x+1)-ln(x-1)]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=ln(x^{2})-[ln(x+1)^{2}+ln(x-1){}^{2}]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=ln\left[\frac{x}{(x+1)\times(x-1)}{}^{2}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=ln\left(\frac{x^{2}}{x^{2}-1}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection*
Problem 42:
\end_layout

\begin_layout Standard
Find the limit:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lim_{x\to5^{+}}ln\left[\frac{x}{\sqrt{x-4}}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=ln\left[\frac{5}{\sqrt{5-4}}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=ln(5)
\]

\end_inset


\end_layout

\begin_layout Subsection*
Problem 62:
\end_layout

\begin_layout Standard
Find the derivative of the function:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y=ln(ln(x))
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y^{\prime}=\frac{\frac{d}{dx}(ln(x))}{ln(x)}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y^{\prime}=\frac{\frac{1}{x}}{ln(x)}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y^{\prime}=\frac{1}{x\times ln(x)}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Problem 104:
\end_layout

\begin_layout Standard
Use logarithmic differentiation:
\end_layout

\begin_layout Standard
\begin_inset Formula $y=\sqrt{\frac{x^{2}-1}{x^{2}+1}}$
\end_inset

, in the region where 
\begin_inset Formula $x>1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset VSpace medskip
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y=\sqrt{\frac{x^{2}-1}{x^{2}+1}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
ln(y)=ln\left[\sqrt{\frac{x^{2}-1}{x^{2}+1}}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
ln(y)=\frac{1}{2}\times[ln(x^{2}-1)-ln(x^{2}+1)]
\]

\end_inset


\end_layout

\begin_layout Standard
Differentiate each side:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{y^{\prime}}{y}=\frac{1}{2}\times\left[\frac{\frac{d}{dx}(x^{2}-1)}{x^{2}-1}-\frac{\frac{d}{dx}(x^{2}+1)}{x^{2}+1}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y^{\prime}=\frac{y}{2}\times\left[\frac{2x}{x^{2}-1}-\frac{2x}{x^{2}+1}\right]
\]

\end_inset


\end_layout

\end_body
